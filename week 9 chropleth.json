{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "title": "World Freedom Index Classification",
  "width": 800,
  "height": 600,
  "projection": {"type": "equirectangular"},
  "data": {
    "url": "https://raw.githubusercontent.com/Emily0410/FIT3179/main/custom.geo%20(1).json",
    "format": {"type": "topojson", "feature": "custom"}
  },
  "transform": [
    {
      "lookup": "properties.name",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/Emily0410/FIT3179/main/hm%20week%209.csv"
        },
        "key": "Country",
        "fields": ["PR"]
      }
    },
    {"calculate": "datum.PR + 0.1", "as": "actual PR"}
  ],
  "mark": {"type": "geoshape"},
  "encoding": {
    "color": {"field": "actual PR", "type": "quantitative"},
    "tooltip": [
      {"field": "properties.name", "type": "nominal", "title": "Country"},
      {"field": "actual PR", "type": "quantitative"}
    ]
  },
  "config": {}
}